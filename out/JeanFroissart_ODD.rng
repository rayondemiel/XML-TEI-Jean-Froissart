            <group>
               <ref name="persName"/>
               <ref name="persName"/>
            </group>
            <ref name="birth"/>
            <ref name="death"/>
            <optional>
               <ref name="note"/>
            </optional>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-person-typeVal-constraint-rule-15">
