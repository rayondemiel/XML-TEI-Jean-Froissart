         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-locus-locusVal-constraint-rule-12">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    context="tei:locus[@from]">
               <s:assert test="matches(@from,'\d{1,3}(v|r)')"> La valeur doit indiquer le numéro de folio (entre 0 et 999) et si recto ou verso</s:assert>
            </s:rule>
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    context="tei:locus[@to]">
               <s:assert test="matches(@to,'\d{1,3}(v|r)')"> La valeur doit indiquer le numéro de folio (entre 0 et 999) et si recto ou verso</s:assert>
            </s:rule>
